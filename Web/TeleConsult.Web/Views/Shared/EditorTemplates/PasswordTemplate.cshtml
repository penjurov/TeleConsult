@model string

<div class="form-group row">
    @Html.LabelFor(m => m, htmlAttributes: new { @class = "col-lg-4 col-xs-5 modal-label" })
    <div class="col-lg-8 col-xs-7">
        @Html.TextBoxFor(m => m, new { @class = "form-control", type="password" })
        @Html.ValidationMessageFor(m => m, "", new { @class = "text-danger" })
    </div>
</div>